{
  "type": "module",
  "name": "backend",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build:ts && tsc -p test/tsconfig.json && FASTIFY_AUTOLOAD_TYPESCRIPT=1 node --test --experimental-test-coverage --loader ts-node/esm test/**/*.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "tsc && tsc-alias",
    "watch:ts": "tsc -w",
    "dev": "npm run build:ts && concurrently -k -p \"[{name}]\" \"tsc-alias -w\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.0",
    "@fastify/csrf-protection": "^7.0.2",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.0.4",
    "@fastify/multipart": "^9.0.3",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/rate-limit": "^10.2.2",
    "@fastify/redis": "^7.0.2",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/websocket": "^11.0.2",
    "@libsql/client": "^0.14.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "await-to-ts": "^1.0.6",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.40.0",
    "drizzle-zod": "^0.7.0",
    "fastify": "^5.0.0",
    "fastify-bcrypt": "^1.0.1",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.2",
    "paseto": "^3.1.4",
    "pg": "^8.13.3",
    "ts-pattern": "^5.6.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.11",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.30.5",
    "fastify-tsconfig": "^2.0.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "^5.2.2"
  }
}
